@charset "utf-8";

/* --- 텍스트형 컨텐츠 --- */
.textsContsWrapper {position:relative; padding:0 0 1.5rem; }
.textsContsWrapper:before {content:''; position:absolute; top:0; left:0; right:0; width:100%; height:3px; background:#1b3e63;}
.textsContsWrapper .innerWrapper {padding:1.5rem 0; font-size:1.4rem;}
.textsContsWrapper > .contsSubTit {padding:1.5rem 1.5rem 1.3rem; font-size:1.4rem; border-bottom:1px solid #dfdfdf; background:#fbfbfb;}
.textsContsWrapper + .btnArea {margin-top:0;}

/* 후원안내 */
.textsContsWrapper.aboutDonation {margin-top:50px;}
.aboutDonation .conBox {width:670px;}
.aboutDonation .innerWrapper {padding-bottom:70px; background:#fff url(../../common/images/bg_about_donation.png) 100% 100% no-repeat;}
.aboutDonation .infobox {width:530px; margin-top:20px; padding:24px 20px; box-sizing:border-box; line-height:160%; background:#f5f5f5; border-radius:10px;}
.aboutDonation .infobox .textPara_highlight {margin-top:0}

/* 인사말 */
.greetingWrapper > p {font-size:1.3rem; line-height:160%;}
.greetingWrapper > p + p {margin-top:1.5rem;}
.greetingWrapper {position:relative;}
.greetingWrapper .imgWrapper {display:block; overflow:hidden; margin:0 -1rem;}
.greetingWrapper .bgIMG {width:100%; height:auto; margin-bottom:2.5rem;}
.greetingWrapper .bottomIMG {width:100%; height:auto; margin-bottom:2.5rem;}
.greetingWrapper .topBG {position:absolute; top:1.5rem; right:1rem; height:290px; box-sizing:border-box; padding:3rem 1rem 0 0; text-align:right;}
.greetingWrapper .topBG > strong {display:block; font-size:2rem; color:#1d1d1d;}
.greetingWrapper .topBG > p {margin-top:1.5rem; line-height:150%; color:#1b3e63; font-weight:700; }
.greetingWrapper .topBG > p em {color:#009944;}

/* 홈페이지 이용방법 */
.textsContsWrapper.emailCollection .innerWrapper {padding-bottom:50px;}
.textsContsWrapper.emailCollection .innerWrapper > p {line-height:150%;}
.djidWrapper > p {font-size:1.3rem; line-height:160%;}
.djidWrapper > p + p {margin-top:1.5rem;}
.djidWrapper {position:relative;}
.djidWrapper .imgWrapper {position:relative;display:block; overflow:hidden; margin:0 -1rem;}
.djidWrapper .bgIMG { width:100%; height:auto; margin-bottom:2.5rem;}
.djidWrapper .topBG {position:absolute; top:7.5rem; width:100%; height:325px; box-sizing:border-box; padding:7re 4rem 0 0; text-align:center;}
.djidWrapper .topBG > strong {display:block; font-size:2rem; color:#1d1d1d; font-weight:700;}
.djidWrapper .topBG > strong em {color:#004ea2; font-weight:700;}
.djidWrapper .topBG > p {margin-top:2.5rem; line-height:150%; font-weight:700; }
.djidWrapper .topBG > p em {color:#3387e0;}

/* --- 조직도 --- */
.organizationChart {position:relative; height:28rem; margin-bottom:2rem; border-bottom:1px dotted #7f7f7f;}
.organizationChart * {box-sizing:border-box;}
.organizationChart .organText {position:relative; display:table; width:100%; height:3rem; font-size:1.2rem; line-height:2.8rem; color:#2a2a2a; background:#ededed; border:1px solid #dedede; text-align:center;}
.organizationChart .organText > span {display:table-cell; vertical-align:middle;}

.organizationChart .chartDepth1 > li {float:left; width:33%;}
.organizationChart .chartDepth1 > li:nth-of-type(2) {width:34%; padding:0 2%;}
.organizationChart .chartDepth3 > li {float:left; width:33%; height:6.5rem;}
.organizationChart .chartDepth3 > li .organText {height:100%}
.organizationChart .chartDepth3 > li:nth-of-type(1) .organText {height:3.1rem}
.organizationChart .chartDepth3 > li:nth-of-type(1) .organText+.organText {margin-top:.2em;}
.organizationChart .chartDepth3 > li:nth-of-type(2) {width:34%; padding:0 2%;}


.chartDep1Box {position:absolute; top:3.8rem; left:0; right:0; width:100%;}
.chartDep2Box {position:absolute; top:3.8rem; left:0; right:0; width:100%;}
.chartDep3Box {position:absolute; top:7.3rem; left:0; right:0; width:100%;}
.chartDep4Box {position:absolute; top:3.8rem; left:0; right:0; width:100%;}

.chartDepth3 > li {float:left; width:33%; box-sizing:border-box;}
.chartDepth3 > li:nth-of-type(3) .organText {line-height:140%;}
.chartDepth3 .organText + .organText {margin-top:.5rem;}

.chartDepth4 > li {float:left;}
.chartDepth4 > li:nth-of-type(1) {width:67%; padding-right:2%;}
.chartDepth4 > li:nth-of-type(2) {width:33%;}

.chartDepth5 > li {float:left; box-sizing:border-box;}
.chartDepth5 > li:nth-of-type(1) {width:100%; margin-bottom:.2rem}
.chartDepth5 > li:nth-of-type(2) {width:50%; padding-right:.2%;}
.chartDepth5 > li:nth-of-type(3) {width:50%; padding-left:.2%;}

.organizationChart a.organText {color:#2a2a2a;}
.organizationChart a.organText span > i {display:inline-block; overflow:hidden; width:.5rem; height:.8rem;  padding-left:.5rem; margin-left:.2rem; background:url(../../common/images/btn_organ_arrow.png) no-repeat 0 0; background-size:.5rem auto;}
.organizationChart a.organText.darkBg {background:#1b3e63; color:#fff; border-radius:0; border-color:#1b3e63;}
.organizationChart a.organText.darkBg span > i {background:url(../../common/images/btn_organ_arrow2.png) no-repeat 100% 0; background-size:.5rem auto;}
.organizationChart .bgColor1 {background:#d3daeb; border-color:#b5c0db;}
.organizationChart .bgColor2 {background:#f8e3c3; border-color:#e9d3b2;}
.organizationInfo {display:none;}
.subjectBox .organizationInfo {display:block;}


/* --- 교통정보 --- */

/* 무료순환 버스 */
.freeBusGuide > .textParaList {font-size:1.3rem; color:#1d1d1d;}
.appDownload {text-align:center;}
.appDownload > a {display:inline-block; width:123px;}
