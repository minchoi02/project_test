.welfareTop .homePageGo:before {position:absolute; top:.6rem; left:.5rem; width:2rem;top:.6rem; left:.9rem;}
.welfareSearch .mapView {margin-top:1.5rem;}
.welfareSearch .mapView > img {width:100%; height:auto;}


