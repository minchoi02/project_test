.busTimeTable .busStopBox {position:relative; padding-left:4rem; font-size:1.3rem;}
.busTimeTable .busStopBox > span {position:absolute; top:0; left:0; display:block; width:4rem; text-align:center; padding:1.5rem 0;}
.busTimeTable .busStopBox .stopTime {width:7.5rem; padding:1.5rem .8rem; border-left:1px solid #dfdfdf; text-align:center;}


.homePageGo {overflow:hidden; position:relative; display:block; box-sizing:border-box; width:3rem; height:2.2rem; padding-left:3rem; background:#c99479; border-radius:2.5rem; color:#fff;}
