p.copy {
	font-size: 22px;
	font-weight: bold;
	text-align: center;
	line-height: 150%;
	margin: 0px 0px 60px 0px;
}
p.copy span.ldquo {
	background: url(../imgaes/common/t_ldquo.png) no-repeat left top;
	padding: 0px 12px 0px 11px;
	margin: 0px 30px 0px 0px;
}
p.copy span.rdquo {
	background: url(../imgaes/common/t_rdquo.png) no-repeat left top;
	padding: 0px 12px 0px 11px;
	margin: 0px 0px 0px 30px;
}
p.box_bank {
	color: #f47420;
	font-weight: bold;
	text-align: center;
	border: 1px solid #f47420;
	padding: 10px 0px 10px 10px;
	background: url(../imgaes/sub/02/bg_bank.png) no-repeat 170px center;
}
div.box_tel {
	background: url(../imgaes/sub/02/bg_tel_s.png) no-repeat right bottom;
}
p.fL {
	float: left;
}
p.fR {
	float: right;
}
p.f_orange {
	font-size: 15px;
	color: #f47420;
	font-weight: bold;
	margin: 0px 0px 40px 0px;
}
/* form_01*/
table.form_01 {
	width: 770px;
	border-top: 3px solid #476b1f;
	margin: 0px 0px 60px 0px;
}
table.form_01 tbody tr {
	border-bottom: 1px solid #ebebeb;
}
table.form_01 tbody tr th {
	background: #f9f9f6;
	font-size: 15px;
	font-weight: bold;
	text-align: left;
	padding: 15px 0px 15px 20px;
	border-right: 1px solid #ebebeb;
}
table.form_01 tbody tr th.tac {
	background: #f9f9f6;
	font-size: 15px;
	font-weight: bold;
	text-align: left;
	padding: 15px 0px 15px 40px;
	border-right: 1px solid #ebebeb;
}
table.form_01 tbody tr td {
	padding: 0px 20px;
	color: #555555;
	font-size: 15px;
	line-height: 150%;
}
