/* --- 회원가입/회원관리 --- */
body.member {background: #f8f8f8;}
.contents.member {float: none; width: 100%; margin-top: 80px}
.member .contents h2 {margin-bottom: 20px;}
.member .contents h2 img {display: block;}
.member .innerBoxMem {width: 700px; padding: 25px 20px 30px; border: 1px solid #dedede; border-top: 2px solid #1b3e63; box-sizing: border-box;background: #fff; }
.member .innerBoxMem.modify {margin: 0 auto}
.member .innerBoxMem h3 {font-size: 18px; font-weight: 700; color: #1d1d1d; margin-bottom: 10px;}
.member .innerBoxMem.login {padding: 60px 110px;}
.member .innerBoxMem.login fieldset {position: relative; margin: 25px 0 40px; padding-bottom: 35px; border-bottom: 1px dotted #ccc;}
.member .innerBoxMem fieldset span {display: block; margin-right: 112px; margin-bottom: 10px;}
.member .innerBoxMem fieldset span label {display: inline-block; width: 66px; line-height: 35px; height: 35px; font-size: 14px; font-weight: 700;}
.member .innerBoxMem fieldset span input {width: 290px; height: 40px;}
.member .innerBoxMem fieldset p {padding: 0 15px 0 70px; overflow: hidden;}
.member .innerBoxMem fieldset p a {display: block; float: right; background: url(/assets/user/images/bul_arrow.png) no-repeat 100% 4px; padding-right: 10px;}
.member .innerBoxMem fieldset input[type="button"] {position: absolute; top: 0; right: 9px; width: 100px; height: 90px;	font-size: 15px; font-weight: 700; background: #1b3e63; color: #fff;}
