.facTable tr td .tit {font-weight:700; font-size:16px; padding:0 15px 0 0; margin-right:8px; background:url(/assets/user/images/bg_list01.png) no-repeat right 5px;}
.facTable tr td dl dt {font-weight:700;padding:5px 0 7px 0}
.facTable tr td dl dd span {font-weight:700; padding:0 0 0 15px; margin-left:8px; background:url(/assets/user/images/bg_list01.png) no-repeat left 3px;}
.facTable tr td dl dd span:first-child {background:none;padding-left:0; margin-left:0;}
.facTable tr th {border-bottom:1px solid #ebebeb;text-align:center;}
.facTable tr th span {display:inline-block; width:30px; height:30px; text-indent: -999px; background:url(/assets/user/images/facility_ov.png) no-repeat;
border-radius:30px;-moz-border-radius:30px;-webkit-border-radius:30px;-ms-border-radius:30px;-khtml-border-radius:30px;-o-border-radius:30px;}
