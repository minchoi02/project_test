@charset "utf-8";

/* --- 조직도 --- */
.organizationChart {position: relative; height: 400px; width: 820px; margin: 65px auto 0;}
.organizationChart .organText {position: relative; display: block; width: 150px; height: 35px; font-weight: 700; text-align: center; border-radius: 20px; line-height: 34px; background: #d9d9d9; color: #2a2a2a;}
.organizationChart a.organText {padding-right: 10px; color: #2a2a2a;}
.organizationChart a.organText:after {content: ''; position: absolute; top: 9px; right: 10px; display: block; width: 17px; height: 17px; background: url(/assets/user/images/btn_organ_arrow.png) no-repeat 100% 0;}
.organizationChart a.organText.darkBg {background: #1b3e63; color: #fff; border-radius: 0;}
.organizationChart a.organText.darkBg:after {background: url(/assets/user/images/btn_organ_arrow2.png) no-repeat 100% 0;}
.organizationInfo {display: none;}

.chartDep1Box {position:absolute; top:70px; left:0; width:100%;}
.chartDep2Box {position:absolute; top:70px; left:0; width:100%;}
.chartDep3Box {position:absolute; top:70px; left:0; width:100%;}
.chartDep4Box {position:absolute; top:70px; left:0; width:100%;}
.organizationChart .chartDepth1 > li > .organText,
.organizationChart .chartDepth2 > li > .organText,
.organizationChart .chartDepth3 > li > .organText,
.organizationChart .chartDepth4 > li > .organText,
.organizationChart .chartDepth5 > li > .organText {position:absolute; top:0;}
.organizationChart .chartDepth1 > li:nth-of-type(1) > .organText {left:25%;}
.organizationChart .chartDepth1 > li:nth-of-type(2) > .organText {left:50%; background:#d8deee;}
.organizationChart .chartDepth1 > li:nth-of-type(3) > .organText {left:75%;}
.organizationChart .chartDepth2 > li:nth-of-type(1) > .organText {left:50%;}
.organizationChart .chartDepth2 > li:nth-of-type(2) > .organText {left:75%;}
.organizationChart .chartDepth3 > li:nth-of-type(1) > .organText {left:0; background:#eed3ac;}
.organizationChart .chartDepth3 > li:nth-of-type(2) > .organText {left:25%;}
.organizationChart .chartDepth3 > li:nth-of-type(3) > .organText {left:50%;}
.organizationChart .chartDepth3 > li:nth-of-type(4) > .organText {left:75%; width:200px;}
.organizationChart .chartDepth4 > li:nth-of-type(1) > .organText {left:50%;}
.organizationChart .chartDepth4 > li:nth-of-type(2) > .organText {left:75%;}
.organizationChart .chartDepth5 > li:nth-of-type(1) > .organText {left:25%; top:-15px;}
.organizationChart .chartDepth5 > li:nth-of-type(2) > .organText {left:30%; top:75px;}
.organizationChart .chartDepth5 > li:nth-of-type(3) > .organText {left:72%; top:75px;}

.organizationChart .chartDep4Box:before {content:''; position:absolute; top:39px; left:39.9%; width:343px; height:1px; background:#ccc; z-index:-1;}
.organizationChart .organText.horizonlLine:before {content:''; position:absolute; top:15px; left:50%; height:1px; background:#ccc; z-index:-1;}
.organizationChart .organText.verticalLine:before {content:''; position:absolute; top:15px; left:50%; width:1px; background:#ccc; z-index:-1;}
.organizationChart .chartDepth1 .organText.verticalLine:before {height:304px;}
.organizationChart .chartDepth5 .organText.verticalLine:before {top:auto; bottom:15px; height:56px}
.organizationChart .chartDepth1 .organText.horizonlLine:before {width:350px;}
.organizationChart .chartDepth3 .organText.horizonlLine:before {width:720px;}
.organizationChart .chartDepth4 .organText.horizonlLine:before {width:250px;}
.organizationChart .chartDepth5 .organText.horizonlLine:before {width:206px;}

.organizationInfo .organtit {font-size:15px; font-weight:700; position:relative; padding:0 0 0 15px; }
.organizationInfo .organtit:before {content:'';position:absolute; top:5px; left:0; width:4px; height:4px; border:2px solid #004ea1; border-radius:6px;}
.organizationInfo .organcon01 {height:350px; background:url(/assets/user/images/federation_01.png) no-repeat 0 15px; padding:70px 0 0 300px; box-sizing: border-box;}
.organizationInfo .organcon01 .name {font-size:25px; font-weight:700; position:relative; padding:0; margin:0 0 25px 0;}

.organList{width:890px; margin:20px 0 0 20px;border-top:2px solid #1b3e63;}
.organList .type05{font-size:14px; border-right:1px solid #dfdfdf; }
.organList .type05 tbody td:first-child{border-left:1px solid #dfdfdf;}
.organList .type05 tbody td{padding: 10px 0; border-right:1px solid #dfdfdf; border-bottom:1px solid #dfdfdf; font-size:14px; text-align:center; vertical-align:top; line-height:1.4;}
.organList .type05 .bg01{background:#f6f6f6;}

/* --- 텍스트형 컨텐츠 --- */
.textsContsWrapper {position: relative; padding: 0 20px 20px; border: 1px solid #dfdfdf; border-top: 0; background: #fbfbfb;}
.textsContsWrapper:before {content: ''; position: absolute; top: 0; left: 0; right: 0; width: 100%; height: 3px; background: #1b3e63;}
.textsContsWrapper .innerWrapper {padding: 30px 20px; margin-top: 20px; font-size: 14px; color: #1d1d1d; border: 1px solid #dfdfdf; background: #fff;}
.textsContsWrapper > .contsSubTit {padding-top: 28px;}

/* 후원안내 */
.textsContsWrapper.aboutDonation { margin-top: 50px;}
.aboutDonation .conBox {width: 670px;}
.aboutDonation .innerWrapper {padding-bottom: 70px; background: #fff url(/assets/user/images/bg_about_donation.png) 100% 100% no-repeat;}
.aboutDonation .infobox {width: 530px; margin-top: 20px; padding: 24px 20px; box-sizing: border-box; line-height: 160%; background: #f5f5f5; border-radius: 10px;}
.aboutDonation .infobox .textPara_highlight {margin-top: 0}

/* 인사말 */
.greetingWrapper > p {font-size:15px; line-height:160%;}
.greetingWrapper .topBG {position: relative; height:290px; box-sizing:border-box; padding:70px 30px 0 0; margin-bottom:40px; text-align:right; background:url(/assets/user/images/bg_greeting_top.png) 0 0 no-repeat;}
.greetingWrapper .topBG > strong {display:block; font-size:30px; color:#1d1d1d;}
.greetingWrapper .topBG > strong em {color:#009944; font-weight:700;}
.greetingWrapper .topBG > p {margin-top:15px; color:#1b3e63;font-weight:700; font-size:15px;}
.greetingWrapper .topBG > p em {color:#3387e0;}

.greetingWrapper .bottomBG {height:430px; box-sizing:border-box; padding:0 0 0 300px;background:url(/assets/user/images/bg_greeting_bottom.png) 0 0 no-repeat;}
.greetingWrapper .bottomBG > p {width:600px;float:left;font-size:15px; line-height:160%;}
.greetingWrapper .bottomBG > p + p {margin-top:15px;}

/* 홈페이지 이용방법 */
.textsContsWrapper.emailCollection .innerWrapper {padding-bottom: 50px;}
.textsContsWrapper.emailCollection .innerWrapper > p {line-height: 150%;}.djidWrapper {}
.djidWrapper .bg01, .djidWrapper .bg02, .djidWrapper .bg03 {width:930px; height:200px; margin:0 0 30px 0;}
.djidWrapper dt {width:270px; height:200px; display:table-cell; float:left; text-align:center; font-size:24px; font-weight:700; box-sizing: border-box;}
.djidWrapper .bg01 dt, .djidWrapper .bg03 dt {padding:85px 0 0 0;}
.djidWrapper .bg02 dt {padding:75px 0 0 0;}
.djidWrapper dd {font-size:15px; font-weight:700; line-height:1.5;}
.djidWrapper .bg02 dd, .djidWrapper .bg03 dd {padding:75px 0 0 0;}
.djidWrapper .bg01 dd {padding:65px 0 0 0;}
.djidWrapper .btxt {color:#004ea2;}
.djidWrapper .bg01 {background:url(/assets/user/images/bg_h01.png) 0 0 no-repeat;}
.djidWrapper .bg02 {background:url(/assets/user/images/bg_h02.png) 0 0 no-repeat;}
.djidWrapper .bg03 {background:url(/assets/user/images/bg_h03.png) 0 0 no-repeat;}

/* ----- 교통정보 ----- */
.trafficInformation .tabContBox .textRight {font-size: 12px; margin: 5px 0;}
.timetableInquiry {position: relative;}
.timetableInquiry .btn_inquiry {position: absolute; top: 229px; right: 48px; display: inline-block;}

/* 버스 시간표 */
.busTimeTable {overflow: hidden; border: 1px solid #dfdfdf; border-top: 3px solid #222;}
.busTimeTable > li {float: left; width: 33%; box-sizing: border-box; border-top: 1px solid #dfdfdf;}
.busTimeTable > li:nth-of-type(3n-1) {width: 34%; border-left: 1px solid #dfdfdf; border-right: 1px solid #dfdfdf;}
.busTimeTable > li:nth-of-type(-n+3) {border-top: 0;}
.busTimeTable .busStopBox {display: table;width: 100%;background: #f9f9fa;}
.busTimeTable .busStopBox > * {display: table-cell;padding: 15px;box-sizing: border-box;font-size: 14px;  }
.busTimeTable .busStopBox > span {width: 40px; text-align: center; padding: 15px 0; background: #ecf6fb; border-right: 1px solid #dfdfdf;}
.busTimeTable .busStopBox .stopTime {float: right; width: 60px; text-align: right;/*  display:none; */}
.busTimeTable .rowEven .busStopBox {background: #fff;}

/* 버스 노선 */
.busRouteWrapper {margin-bottom: 40px;}
.currentInfoBox {position: absolute; top: 50%; left: 50%; width: 440px; height: 30px; margin: -10px 0 0 -220px; text-align: center;}
.currentInfoBox .statusTxts {display: table; height: 100%; margin: 0 auto;}
.currentInfoBox .statusTxts > * {display: table-cell; vertical-align: middle;}
.currentInfoBox .statusTxts > span {font-size: 18px; color: #111; font-weight: 700;}
.currentInfoBox .statusTxts em {display: inline-block; margin-left: 18px; padding: 6px 26px; font-size: 14px; color: #fff; border-radius: 14px; background: #969696;}
.currentInfoBox .statusTxts.waiting em {background: #7fb6b4;}
.currentInfoBox .statusTxts.finished em {background: #dd526a;}
.currentInfoBox .statusTxts.runing em {background: #004ea2;}

.busRoute {display: none; background: #f9f9f9; height: 450px; box-sizing: border-box; padding-top: 55px;}
.busRoute > .busStopList {position: relative; width: 720px;	height: 310px; margin: 0 auto; background: #fff; border-radius: 20px; border: 1px solid #c6c9cc;}
.busRoute > .busStopList > ol {border: 0 solid blue; box-sizing: border-box; height: 310px;}
.busRoute > .busStopList li {position: absolute;}
.busRoute li>strong {position: absolute; top: 20px; left: -29px; display: block; width: 64px; text-align: center; font-size: 12px; letter-spacing: -1px; line-height: 130%;}
.busRoute li:before {content: ''; position: absolute; top: -6px; left: 0; display: block; width: 12px; height: 12px; border-radius: 6px; background-color: #c6c9cc;}

.busStopList .iconDrivingBus {position: absolute; left: 0; width: 100%; height: 2px; z-index: 10;}
.busStopList .iconDrivingBus:before {content: ''; position: absolute; left: 0;top: -36px; z-index: 50; display: block; width: 30px; height: 48px; margin-left: -15px; box-sizing: border-box; background: url(/assets/user/images/icon_bus.png) 0 0 no-repeat;}
.busStopList .distance_1.iconDrivingBus:before {left: 25%;}
.busStopList .distance_2.iconDrivingBus:before {left: 50%;}
.busStopList .distance_3.iconDrivingBus:before {left: 75%;}
.busStopList .distance_4.iconDrivingBus:before {left: 100%;}

/* 모서리 - 우측 상단 */
.busStopList .cornerRT .iconDrivingBus {width: 70%;}
.busStopList .cornerRT .distance_1.iconDrivingBus:before {left: 25%;}
.busStopList .cornerRT .distance_2.iconDrivingBus:before {left: 70%;}
.busStopList .cornerRT .distance_3.iconDrivingBus, 
.busStopList .cornerRT .distance_4.iconDrivingBus {position: absolute; left: 50px; top: 0; width: 2px; height: 45px;}
.busStopList .cornerRT .distance_3.iconDrivingBus:before {top: -50%; right: 0;}
.busStopList .cornerRT .distance_4.iconDrivingBus:before {top: 0; right: 0;}

/* 위에서 아래 */
.busRoute > .busStopList li.toBottom:before {left: -6px; top: 0;}
.busStopList .toBottom .iconDrivingBus {width: 2px; height: 100%;}
.busStopList .toBottom .iconDrivingBus:before {left: 0;bottom: 100%; top: auto;}
.busStopList .toBottom .distance_1.iconDrivingBus:before {bottom: 75%;}
.busStopList .toBottom .distance_2.iconDrivingBus:before {bottom: 50%;}
.busStopList .toBottom .distance_3.iconDrivingBus:before {bottom: 25%;}
.busStopList .toBottom .distance_4.iconDrivingBus:before {bottom: 0;}

/* 모서리 - 우측 하단 */
.busRoute > .busStopList li.cornerRB:before {left:-6px; top:0;}
.busStopList .cornerRB .iconDrivingBus {left:0; width:2px; height:100%; margin-bottom:-6px;}
.busStopList .cornerRB .iconDrivingBus:before {top:auto; left:-15px;}
.busStopList .cornerRB .distance_1.iconDrivingBus:before {bottom:25%;}
.busStopList .cornerRB .distance_2.iconDrivingBus:before {bottom:0;}
.busStopList .cornerRB .distance_3.iconDrivingBus {left:-23px; width:25px; height:2px; bottom:0;}
.busStopList .cornerRB .distance_4.iconDrivingBus {left:-43px; width:45px; height:2px; bottom:0;}
.busStopList .cornerRB .distance_3.iconDrivingBus:before,
.busStopList .cornerRB .distance_4.iconDrivingBus:before {top:-36px;}

/* 오른쪽에서 왼쪽으로 */
.busRoute > .busStopList li.toLeft {margin-left:0;}
.busStopList .toLeft .iconDrivingBus {left:-100%; width:100%; height:2px; margin-left:12px; /* background:red */}
.busStopList .toLeft .iconDrivingBus:before {left:auto; right:-15px;}
.busStopList .toLeft .distance_1.iconDrivingBus:before {left:75%;}
.busStopList .toLeft .distance_2.iconDrivingBus:before {left:50%;}
.busStopList .toLeft .distance_3.iconDrivingBus:before {left:25%;}
.busStopList .toLeft .distance_4.iconDrivingBus:before {left:0;}

/* 모서리 - 좌측 하단 */
.busStopList .cornerLB .iconDrivingBus {left:auto; right:100%; width:45px; margin-left:0; height:2px;}
.busStopList .cornerLB .iconDrivingBus:before {left:100%}
.busStopList .cornerLB .distance_1.iconDrivingBus:before {left:50%;}
.busStopList .cornerLB .distance_2.iconDrivingBus:before {left:0;}
.busStopList .cornerLB .distance_3.iconDrivingBus,
.busStopList .cornerLB .distance_4.iconDrivingBus {left:-45px; top:-52px; right:auto; width:2px; height:52px;}
.busStopList .cornerLB .distance_3.iconDrivingBus:before {left:0; top:-15%}
.busStopList .cornerLB .distance_4.iconDrivingBus:before {left:0; top:-75%;}

/* 아래에서 위로 */
.busRoute > .busStopList li.toTop:before {top:0;}
.busRoute > .busStopList li.toTop:before {margin-left:-6px;}
.busStopList .toTop .iconDrivingBus {top:-100%; bottom:auto; width:2px; height:100%; margin-top:12px;}
.busStopList .toTop .iconDrivingBus:before {right:0; top:0; margin-top:6px;}
.busStopList .toTop .distance_1.iconDrivingBus:before {top:-25%;}
.busStopList .toTop .distance_2.iconDrivingBus:before {top:-50%;}
.busStopList .toTop .distance_3.iconDrivingBus:before {top:-75%;}
.busStopList .toTop .distance_4.iconDrivingBus:before {top:-100%;}

